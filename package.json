{
  "name": "@brettz9/eslint-plugin",
  "version": "2.1.0",
  "description": "Additional ESLint rules.",
  "author": "Brett Zamir",
  "contributors": [
    "Toru Nagashima"
  ],
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "tsc": "tsc",
    "clean": "rimraf .nyc_output coverage",
    "coverage": "opener coverage/lcov-report/index.html",
    "build": "tsc -p tsconfig-prod.json",
    "lint": "eslint .",
    "mocha": "mocha \"tests/lib/**/*.js\"",
    "c8": "c8 pnpm run mocha",
    "test": "pnpm build && pnpm run --silent lint && pnpm run c8",
    "update": "node scripts/generate-rules",
    "preversion": "run-s clean update test",
    "version": "eslint lib/rules --fix && git add lib",
    "postversion": "git push && git push --tags",
    "watch": "npm run mocha -- --watch --growl"
  },
  "repository": "https://github.com/brettz9/eslint-plugin",
  "keywords": [
    "eslint",
    "eslintplugin",
    "block",
    "scope",
    "instanceof",
    "isArray",
    "static",
    "this",
    "rest",
    "spread",
    "ignore"
  ],
  "bugs": "https://github.com/brettz9/eslint-plugin/issues",
  "homepage": "https://github.com/brettz9/eslint-plugin#readme",
  "peerDependencies": {
    "eslint": ">=7.20.0"
  },
  "devDependencies": {
    "@brettz9/eslint-plugin": "file:",
    "@types/chai": "^4.3.16",
    "@types/eslint": "^8.56.10",
    "@types/estree": "^1.0.5",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.11",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "eslint": "^9.7.0",
    "espree": "^10.1.0",
    "globals": "^15.8.0",
    "mocha": "^10.6.0",
    "npm-run-all": "^4.1.5",
    "opener": "^1.5.2",
    "rimraf": "^5.x",
    "typescript": "^5.5.3"
  }
}
