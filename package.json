{
  "name": "@brettz9/eslint-plugin",
  "version": "1.0.0",
  "description": "Additional ESLint rules.",
  "author": "Brett Zamir",
  "contributors": [
    "Toru Nagashima"
  ],
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "clean": "rimraf .nyc_output coverage",
    "coverage": "opener coverage/lcov-report/index.html",
    "lint": "eslint .",
    "mocha": "mocha \"tests/lib/**/*.js\"",
    "nyc": "nyc npm run mocha",
    "test": "npm run --silent lint && npm run nyc",
    "update": "node scripts/generate-rules",
    "preversion": "run-s clean update test",
    "version": "eslint lib/rules --fix && git add lib",
    "postversion": "git push && git push --tags",
    "watch": "mocha \"tests/lib/**/*.js\" --reporter dot --watch --growl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/eslint-plugin.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "block",
    "scope",
    "instanceof",
    "isArray",
    "static",
    "this",
    "rest",
    "spread",
    "ignore"
  ],
  "bugs": "https://github.com/brettz9/eslint-plugin/issues",
  "homepage": "https://github.com/brettz9/eslint-plugin#readme",
  "peerDependencies": {
    "eslint": ">=6.8.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@brettz9/eslint-plugin": "file:",
    "eslint": "^7.20.0",
    "mocha": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "opener": "^1.5.2",
    "rimraf": "^3.0.2"
  }
}
